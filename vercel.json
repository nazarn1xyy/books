{
    "rewrites": [
        {
            "source": "/flibusta/:path*",
            "destination": "/api/proxy?path=:path*"
        },
        {
            "source": "/api/:match*",
            "destination": "/api/:match*"
        },
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ]
}